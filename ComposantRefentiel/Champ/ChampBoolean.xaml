<local:ChampGenerique 
    x:Class="ComposantReferentiel.Champ.ChampBoolean" 
    x:Name="Root" 
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
    xmlns:local="clr-namespace:ComposantReferentiel.Champ"
    xmlns:converter="clr-namespace:ComposantReferentiel.Converter">
    <local:ChampGenerique.Resources>
        <converter:ConverterBoolean x:Key="converter" />
    </local:ChampGenerique.Resources>
    <StackPanel DataContext="{Binding ElementName=Root, Path=DataContext}">
        <CheckBox Name="checkbox" />
    </StackPanel>
</local:ChampGenerique>
