<local:ChampGenerique 
    x:Class="ComposantReferentiel.Champ.ChampList" 
    x:Name="Root" 
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:ComposantReferentielV2.Champ">
    <StackPanel DataContext="{Binding ElementName=Root, Path=DataContext}">
        <ComboBox 
            Name="combobox" 
            ItemsSource="{Binding ElementName=Root, Path=SourceList}" />
    </StackPanel>
</local:ChampGenerique>